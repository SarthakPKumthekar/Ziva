// the main function
global proc ziva_main_goalingDemo(int $newScene)
{
  if (($newScene == 1) && (`about -batch` != 1))
  {
    if (`file -q -anyModified`)
    {
      $response = `confirmDialog -title "Changes not saved"
        -message "This example will create a new scene. There are unsaved changes in the current scene that will be lost. Proceed?"
        -button "Yes"
        -button "Cancel"
        -defaultButton "Yes"
        -cancelButton "Cancel"
        -dismissString "Cancel"`;

      if ( $response == "Cancel" )
        return;
    }

    file -f -new;
  }

  // force to start at frame 1 as usual, otherwise in later 2023 Maya any sence starts at a negative frame by default,
  // this is a workaround for the above issue, the long-term solution should come from Autodesk.
  currentTime 1;

  // load the geometry of the fish (two identical copies), setup the skeleton rig and animation of the first fish
  $goalingSceneFilePath = getDemoAssetPath("fishGeometry.ma");
  file -import -type "mayaAscii" -ignoreVersion -ra true -mergeNamespacesOnClash false -defaultNamespace -options "v=0;p=17;f=0" -pr $goalingSceneFilePath;

  // show the texture
  displayStyle -textured;

  // make a solver node
  ziva -s;

  // adjust the solver scale, to generate more interesting deformations
  setAttr "zSolver1.scaleZ" 1;
  setAttr "zSolver1.scaleX" 1;
  setAttr "zSolver1.scaleY" 1;

  // disable gravity
  setAttr "zSolver1.gravityY" 0.0;

  // decrease the number of solver Newton iterations to 1
  // this increases simulation speed; the sim is still stable
  setAttr "zSolver1.maxNewtonIterations" 1;

  // enable collision detection
  setAttr "zSolver1.collisionDetection" 1;

  // convert the first fish into a bone
  select -r inputSkinnedMesh;
  ziva -b;

  // convert the second fish into a tissue
  select -r outputZivaMesh;
  ziva -t;
  setAttr "zTissue1.selfCollisions" 1; // enable self-collisions
  setAttr "zMaterial1.youngsModulus" 24; // adjust stiffness (fish flexibility)
  setAttr "zMaterial1.youngsModulusExp" 3;

  // make a new material, to make the tail fin stiffer
  ziva -m;
  setAttr "zMaterial2.youngsModulus" 48; // adjust stiffness (tail fin flexibility) -- make it 2x stiffer than the rest of the fish
  setAttr "zMaterial2.youngsModulusExp" 3;
  // paint the new material zero (onto the embedded triangle mesh), so that it is transparent (i.e., disabled)
  for($i=0; $i<7106; $i++) // 7106 is the number of vertices in the fish triangle mesh
    setAttr zMaterial2.weightList[0].weights[$i] 0;
  // now, paint the tail fin vertices 1, so that the new material applies in that region
  // these vertex ranges were created in Maya, using lasso selection
  int $tailFinVerticesRangeStart[] = { 31, 42, 95, 199, 368, 769, 773, 775, 783, 786, 788, 795, 798, 800, 807, 810, 812,
                                       1438, 1441, 1450, 1453, 1462, 1465, 1474, 1477, 1484, 3017, 3030, 3068, 3080,
                                       3116, 3128, 3164, 3176, 5685, 5691, 5694, 5723, 5727, 5733, 5739, 5742, 5771,
                                       5781, 5782, 5787, 5790, 5819, 5823, 5829, 5835, 5838, 5865, 5868, 5870 };
  int $tailFinVerticesRangeEnd[] = { 31, 59, 104, 249, 408, 769, 773, 781, 783, 786, 793, 795, 798, 805, 807, 810, 965,
                                     1438, 1447, 1450, 1459, 1462, 1471, 1474, 1482, 1615, 3023, 3061, 3073, 3109,
                                     3121, 3157, 3169, 3789, 5689, 5691, 5719, 5725, 5727, 5737, 5739, 5767, 5773,
                                     5781, 5785, 5787, 5815, 5821, 5823, 5833, 5835, 5861, 5866, 5868, 6421 };
  for($i=0; $i<size($tailFinVerticesRangeStart); $i++)
  {
    for($j=$tailFinVerticesRangeStart[$i]; $j <= $tailFinVerticesRangeEnd[$i]; $j++)
    setAttr zMaterial2.weightList[0].weights[$j] 1;
  }

  // make a new material, to make the dorsal fin softer
  ziva -m;
  setAttr "zMaterial3.youngsModulus" 24; // adjust stiffness (dorsal fin flexibility) -- make it 10x softer than the rest of the fish
  setAttr "zMaterial3.youngsModulusExp" 2;
  // paint the new material zero (onto the embedded triangle mesh), so that it is transparent (i.e., disabled)
  for($i=0; $i<7106; $i++) // 7106 is the number of vertices in the fish triangle mesh
    setAttr zMaterial3.weightList[0].weights[$i] 0;
  // now, paint the dorsal fin vertices 1, so that the new material applies in that region
  // these vertex ranges were created in Maya, using lasso selection
  int $dorsalFinVerticesRangeStart[] = { 32, 67, 89, 173, 177, 180, 183, 186, 282, 343, 347, 351, 354, 357,
                                         439, 661, 673, 678, 688, 697, 700, 703, 712, 715, 1098, 1336, 1342,
                                         1352, 1356, 1369, 1372, 1378, 1382, 1392, 1744, 2589, 2600, 2640,
                                         2645, 2654, 2664, 2694, 2702, 2736, 2739, 2747, 2750, 2753, 2760,
                                         2763, 2794, 2797, 2802, 4330, 5274, 5278, 5291, 5300, 5336, 5340,
                                         5350, 5354, 5403, 5406, 5416, 5419, 5436, 5439, 5450, 5458, 5494, 6954 };
  int $dorsalFinVerticesRangeEnd[] = { 41, 69, 94, 173, 177, 180, 184, 198, 292, 343, 347, 351, 354, 367,
                                       445, 661, 673, 678, 688, 697, 700, 704, 712, 767, 1139, 1336, 1342,
                                       1352, 1356, 1369, 1372, 1379, 1384, 1437, 1777, 2589, 2600, 2640,
                                       2645, 2654, 2664, 2694, 2702, 2736, 2739, 2747, 2750, 2755, 2761,
                                       2765, 2794, 2797, 3009, 4497, 5274, 5278, 5291, 5300, 5336, 5340,
                                       5350, 5354, 5403, 5406, 5416, 5420, 5437, 5444, 5456, 5461, 5681, 7105 };
  for($i=0; $i<size($dorsalFinVerticesRangeStart); $i++)
  {
    for($j=$dorsalFinVerticesRangeStart[$i]; $j <= $dorsalFinVerticesRangeEnd[$i]; $j++)
    setAttr zMaterial3.weightList[0].weights[$j] 1;
  }

  // make the mesh more detailed, so that the fins are resolved (as opposed to fused with the fish body)
  // these vertices were selected in the Maya viewport; we make the mesh is 2x refined there (to separate the fish fin from the body)
  // left fin:
  int $refinedVertices[] = { 4276, 1087, 4279, 4269, 1082, 4262, 4265, 4260, 4277, 4272,
                             6920, 6923, 6915, 1737, 6914, 6916, 6890, 1729, 6888, 6891, 6881, 1730 };
  for($i=0; $i<size($refinedVertices); $i++)
  {
    setAttr zTet1.weightList[0].weights[$refinedVertices[$i]] 1;
  }
  setAttr "zTet1.tetSize" 0.1; // decrease overall resolution

  // generate a goal attachment
  // in this way, the tissue fish will be attracted to the motion of the bone fish
  // the attraction only works in the region where the attachment map is painted to 1.0
  // we paint the fish body 1.0, and the fins by 0.0; therefore, the fins evolve according to physics
  ziva_generateGoalAttachment_goalingDemo();

  // set the camera view
  setAttr "persp.translateX" 3.0;
  setAttr "persp.translateY" 4.6;
  setAttr "persp.translateZ" -2.5;
  setAttr "persp.rotateX" 308.0;
  setAttr "persp.rotateY" 125.0;
  setAttr "persp.rotateZ" 0.0;
  setAttr "persp.scaleX" 1.0;
  setAttr "persp.scaleY" 1.0;
  setAttr "persp.scaleZ" 1.0;
  select -r joint1;
  if (`about -batch` != 1)
  {
    viewFit -f 0.4; // center the camera on the joint1, which gives a good view
  }
  setAttr "persp.translateX" 3.0;
  setAttr "persp.translateY" 4.6;
  setAttr "persp.translateZ" -2.5;

  // de-select everything
  select -cl;

  // select the simulation mesh
  select -r outputZivaMesh;

  // add and enable the Ziva cache
  // the simulation will be automatically cached
  ziva -acn;

  playbackOptions -e -minTime 1; // adjust playback to start from frame 1
  playbackOptions -e -maxTime 160; // adjust playback to give 160 frames
  playbackOptions -e -playbackSpeed 0; // set the playback to every frame

  printDemoScriptPath("ziva_main_goalingDemo");
  print "Scene created. The textured fish is simulated. Its body tracks the Maya-keyframed gray fish. This produces secondary motion in the fins. Press \"Play\" to simulate.\n";
}
